    &nbsp;&nbsp;&nbsp;&nbsp;Console.WriteLine(<span class="String">"{0}&nbsp;to&nbsp;decimal&nbsp;is&nbsp;{1}."</span>,<br />
    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;hexa,&nbsp;Convert.ToInt32(hexa,&nbsp;16));<br />
    }
</div>
<hr>

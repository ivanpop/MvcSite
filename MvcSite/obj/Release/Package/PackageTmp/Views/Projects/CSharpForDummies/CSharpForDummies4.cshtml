    &nbsp;&nbsp;&nbsp;&nbsp;}<br />
    &nbsp;&nbsp;&nbsp;&nbsp;<br />
    &nbsp;&nbsp;&nbsp;&nbsp;Console.WriteLine(<span class="String">"Sum&nbsp;of&nbsp;all&nbsp;numbers&nbsp;is&nbsp;{0}."</span>,&nbsp;sum);<br />
    }
</div>

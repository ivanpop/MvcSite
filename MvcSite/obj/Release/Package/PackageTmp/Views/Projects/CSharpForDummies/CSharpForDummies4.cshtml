    &nbsp;&nbsp;&nbsp;&nbsp;<span class="ValueType">int</span>&nbsp;sum&nbsp;=&nbsp;0;<br />
    &nbsp;&nbsp;&nbsp;&nbsp;<br />
    &nbsp;&nbsp;&nbsp;&nbsp;Console.Write(<span class="String">"Enter&nbsp;numbers&nbsp;count:&nbsp;"</span>);<br />
